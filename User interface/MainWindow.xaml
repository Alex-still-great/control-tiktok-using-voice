        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:User_interface"
        mc:Ignorable="d"
        Title="Control Tiktok Using Voice" Height="450" Width="800">
    <Window.Background>#2c2d25</Window.Background>
    <Grid>
        <StackPanel  Background="Black" Margin="0, 0, 0, 0" Panel.ZIndex="0"></StackPanel>
        <TextBlock Text=" Control Tiktok Using Voice"  Foreground="White" FontSize="22" FontWeight="Bold" Margin="177,25,-177,-26"  ></TextBlock>
        <Button  Content="Start listening" Width="85" Height="25" Margin="43,89,672,320" Click="runcode_Click" Panel.ZIndex="1"/>
        <ListBox x:Name="ListLogs" Height="100" Background="#F0F0F0" Grid.Column="1"/>
    </Grid>
</Window>
